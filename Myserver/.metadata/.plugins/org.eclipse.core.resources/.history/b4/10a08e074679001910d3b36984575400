public class HandlerMap {
 
	private static Map<Short, Class<?>> typeToMsgClassMap;  
	  
	// 根据类型得到对应的消息类的class对象  
	public static Class<?> getMsgClassByType(short type) {  
	    return typeToMsgClassMap.get(type);  
	}  
	
	
